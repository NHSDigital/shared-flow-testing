{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "Client",
    "description": "A client",
    "type": "object",
    "properties": {
      "ip": {
        "description": "Client IP from x-forwarded-for",
        "type": "string"
      },
      "received_start": {
        "description": "The timestamp value specifying when the proxy began receiving the request from the originating client at the ProxyEndpoint.",
        "type": "date-time"
      }
    },
    "required": [ "productId" ]

  }